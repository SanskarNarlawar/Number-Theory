      "source": [
        "def extended_euclid(a,b):\r\n",
        "  if a%b == 0:\r\n",
        "    return b,0,1;\r\n",
        "  else :\r\n",
        "   gcd,x,y = extended_euclid(b,a%b)\r\n",
        "   swap = x\r\n",
        "   x = y\r\n",
        "   y = swap - ((a-a%b)/b)*y\r\n",
        "   return gcd,x,y;\r\n"
      ],
      "execution_count": 1,
      "outputs": []
    }
  ]
